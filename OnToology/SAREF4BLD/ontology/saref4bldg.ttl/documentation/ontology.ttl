              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom <https://w3id.org/def/saref4bldgMeasurement> ;
                owl:onProperty <https://w3id.org/def/saref4bldgimaginaryImpedanceRatio>
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty <https://w3id.org/def/saref4bldgsecondaryCurrentType>
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom <https://w3id.org/def/saref4bldgMeasurement> ;
                owl:onProperty <https://w3id.org/def/saref4bldgapparentPowerMax>
