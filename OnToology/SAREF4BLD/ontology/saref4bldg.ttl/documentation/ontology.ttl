              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty <https://w3id.org/def/saref4bldgendShapeType>
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom <https://w3id.org/def/saref4bldgMeasurement> ;
                owl:onProperty <https://w3id.org/def/saref4bldgnominalDepth>
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
