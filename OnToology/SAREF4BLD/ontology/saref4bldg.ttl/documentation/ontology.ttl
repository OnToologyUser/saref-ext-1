                owl:allValuesFrom xsd:string ;
                owl:onProperty <https://w3id.org/def/saref4bldgrefrigerantClass>
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty <https://w3id.org/def/saref4bldgpowerSource>
              ] ;
