                owl:onProperty <https://w3id.org/def/saref4bldgsecondaryApparentPower>
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom <https://w3id.org/def/saref4bldgMeasurement> ;
                owl:onProperty <https://w3id.org/def/saref4bldgsecondaryCurrent>
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom <https://w3id.org/def/saref4bldgMeasurement> ;
                owl:onProperty <https://w3id.org/def/saref4bldgrealImpedanceRatio>
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom <https://w3id.org/def/saref4bldgMeasurement> ;
