                owl:onProperty <https://w3id.org/def/saref4bldgnominalPowerConsumption>
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom <https://w3id.org/def/saref4bldgMeasurement> ;
