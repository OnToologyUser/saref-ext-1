              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom <https://w3id.org/def/saref4bldgMeasurement> ;
                owl:onProperty <https://w3id.org/def/saref4bldgnominalCapacity>
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom <https://w3id.org/def/saref4bldgMeasurement> ;
