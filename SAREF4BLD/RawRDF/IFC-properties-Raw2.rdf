	<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
</rdf:Description>

<rdf:Description rdf:about="http://saref.linkeddata.es/def/bldg#transformerIsNeutralSecondaryTerminalAvailable">
