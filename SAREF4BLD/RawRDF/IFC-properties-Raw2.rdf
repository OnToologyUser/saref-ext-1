	<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
</rdf:Description>

<rdf:Description rdf:about="http://saref.linkeddata.es/def/bldg#filterOperationTemperatureRangeMin">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
	<rdfs:label xml:lang="en">operation temperature range min</rdfs:label>
	<rdfs:domain rdf:resource="http://saref.linkeddata.es/def/bldg#Filter"/>
	<rdfs:comment xml:lang="en">Allowable operation ambient fluid temperature range. Usually measured in degrees Kelvin (K).</rdfs:comment>
	<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
</rdf:Description>

<rdf:Description rdf:about="http://saref.linkeddata.es/def/bldg#filterFlowRateRangeMax">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
