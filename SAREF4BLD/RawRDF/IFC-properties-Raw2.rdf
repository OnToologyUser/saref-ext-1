	<rdfs:comment xml:lang="en"> Internal volume of condenser (refrigerant side). Usually measured in cubic metre (m3).</rdfs:comment>
	<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
</rdf:Description>

<rdf:Description rdf:about="http://saref.linkeddata.es/def/bldg#condenserInternalWaterVolume">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
	<rdfs:label xml:lang="en">internal water volume</rdfs:label>
	<rdfs:domain rdf:resource="http://saref.linkeddata.es/def/bldg#Condenser"/>
	<rdfs:comment xml:lang="en"> Internal volume of condenser (water side). Usually measured in cubic metre (m3).</rdfs:comment>
	<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
</rdf:Description>

<rdf:Description rdf:about="http://saref.linkeddata.es/def/bldg#condenserNominalHeatTransferArea">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
