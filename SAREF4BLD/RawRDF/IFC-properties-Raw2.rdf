	<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
</rdf:Description>

<rdf:Description rdf:about="http://saref.linkeddata.es/def/bldg#filterFlowRateRangeMin">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
	<rdfs:label xml:lang="en">flow rate range min</rdfs:label>
	<rdfs:domain rdf:resource="http://saref.linkeddata.es/def/bldg#Filter"/>
	<rdfs:comment xml:lang="en">Possible range of fluid flowrate that can be delivered. Usually measured in m3/s.</rdfs:comment>
